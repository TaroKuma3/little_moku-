<h1>Work#index</h1>
<h1><%= "#{@user.name}さんの成果物一覧" %></h1>

  <% @works.each do |work| %>
    <ul>
      <li>
        <%= link_to(work.title, user_work_path(work.user_id, work.id)) %>
      </li>
    </ul>
  <% end %>

<h4><%= link_to("成果物の登録はこちらから対象のMOKUを選択！", user_mokus_path(@user))%></h4>


<p><%= link_to("マイページへ戻る", "/mypage")%></p>
